
@{
    ViewBag.Title = "ViewMembers";
}

@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm())
    {
        <table class="table table-bordered table-hover">
            <tr>
                <th>Membres</th>
            </tr>
            @{
                if (ViewBag.list != null)
                {
                    foreach (string e in ViewBag.list)
                    {
                        <tr>
                            <td>@e</td>
                        </tr>
                    }
                }
            }
        </table>
        <table class="table table-bordered table-hover">
            <tr>
                <th>Bureau</th>
            </tr>
            @{
                if (ViewBag.desk != null)
                {
                    foreach (string e in ViewBag.desk)
                    {
                        <tr>
                            <td>@e</td>
                        </tr>
                    }
                }
            }
        </table>
        <table class="table table-bordered table-hover">
            <tr>
                <th>Président</th>
            </tr>
            @{
                if (ViewBag.pres != null)
                {
                    foreach (string e in ViewBag.pres)
                    {
                        <tr>
                            <td>@e</td>
                        </tr>
                    }
                }
            }
        </table>
    }
}
else
{
    <h3> Il faut être connecté pour faire ça. </h3>
}